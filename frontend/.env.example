# Environment Variables Example
# Copy this file to .env and configure your values

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=30000

# Environment (development, production, staging)
VITE_NODE_ENV=development

# Application Configuration
VITE_APP_NAME=Gold Trading AI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Advanced Gold Trading Bot Dashboard

# Feature Flags (true/false)
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME_UPDATES=true

# UI Configuration
VITE_DEFAULT_THEME=dark
VITE_AUTO_REFRESH_INTERVAL=30000
VITE_CHART_REFRESH_INTERVAL=5000

# Trading Configuration
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_GOLD_SYMBOL=XAUUSD
VITE_MAX_TRADE_AMOUNT=10000
VITE_MIN_TRADE_AMOUNT=100

# Security Configuration
VITE_ENABLE_CSRF_PROTECTION=true
VITE_SESSION_TIMEOUT=3600000

# Analytics & Monitoring (optional)
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ENDPOINT=
VITE_ERROR_REPORTING_ENDPOINT=
